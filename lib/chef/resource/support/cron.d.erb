# Generated by Chef. Changes will be overwritten.
<% if @mailto -%>
MAILTO=<%= @mailto %>
<% end -%>
<% if @path -%>
PATH=<%= @path %>
<% end -%>
<% if @shell -%>
SHELL=<%= @shell %>
<% end -%>
<% if @home -%>
HOME=<%= @home %>
<% end -%>
<% if @random_delay -%>
RANDOM_DELAY=<%= @random_delay %>
<% end -%>
<% @environment.each do |key, val| -%>
<%= key %>=<%= val.to_s.shellescape %>
<% end -%>

<% if @comment -%>
# <%= @comment %>
<% end -%>
<% if @predefined_value -%>
<%= @predefined_value %> <%= @user %> <%= @command %>
<% else -%>
<%= @minute %> <%= @hour %> <%= @day %> <%= @month %> <%= @weekday %> <%= @user %> <%= @command %>
<% end -%>
